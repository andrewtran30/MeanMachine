library(ggplot2)
library(tidyverse)
# Create the dataset
japan_auto <- data.frame(
year = c(1954, 1958, 1962, 1966, 1970, 1973),
production = c(69, 128, 268, 878, 3179, 4471),
exports = c(0, 2, 16, 153, 1090, 2700),
unit_cost = c(100, 90, 75, 55, 40, 35)
)
# Create plot
ggplot(japan_auto, aes(x=year)) +
geom_line(aes(y=production, color="Production"), size=1) +
geom_line(aes(y=exports, color="Exports"), size=1) +
geom_line(aes(y=unit_cost, color="Unit Cost Index"), size=1) +
scale_color_manual(values=c("Production"="blue", "Exports"="green", "Unit Cost Index"="red")) +
labs(title="Japanese Automotive Industry Development (1954-1973)",
x="Year",
y="Vehicles (Thousands)",
color="Metric") +
theme_light() +
theme(
plot.title = element_text(hjust=0.5),
legend.position="bottom"
)
ggsave("japan_auto.png", width=10, height=6)
# Install and load required packages
library(ggplot2)
# Create the dataset
data <- data.frame(
year = c(1954, 1958, 1962, 1966, 1970, 1973),
production = c(69, 128, 268, 878, 3179, 4471),
exports = c(0, 2, 16, 153, 1090, 2700),
unit_cost = c(100, 90, 75, 55, 40, 35)
)
# Create the plot
ggplot(data) +
geom_line(aes(x=year, y=production, color="Production"), size=1) +
geom_line(aes(x=year, y=exports, color="Exports"), size=1) +
geom_line(aes(x=year, y=unit_cost, color="Unit Cost Index"), size=1) +
scale_y_continuous(
name = "Vehicles (Thousands)",
sec.axis = sec_axis(~., name="Unit Cost Index (1954=100)")
) +
scale_color_manual(values=c("Production"="#8884d8",
"Exports"="#82ca9d",
"Unit Cost Index"="#ff7300")) +
labs(
title = "Japanese Automotive Industry Development (1954-1973)",
x = "Year",
color = "Metric"
) +
theme_minimal() +
theme(
legend.position = "bottom",
plot.title = element_text(hjust = 0.5)
)
